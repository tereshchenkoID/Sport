mixin social
	ul.flex.align-middle.social&attributes(attributes)
		li.social__item
			a(href="#").social__link.social__link--twitter
		li.social__item
			a(href="#").social__link.social__link--facebook
		li.social__item
			a(href="#").social__link.social__link--google
		li.social__item
			a(href="#").social__link.social__link--youtube